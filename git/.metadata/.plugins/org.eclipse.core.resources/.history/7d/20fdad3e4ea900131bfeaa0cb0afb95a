package edu.uci.ics.BoardGameServer.Board;

public class GameObject {
	
	private int x;
	private int y;
	private Player player;
	
	public int getX(){
		return this.x;
	}
	
	public void setX(int newX){
		this.x = newX;
	}
	
	public int getY(){
		return this.y;
	}
	
	public void setY(int newY){
		this.y = newY;
	}
	
	public Player getOwner(){
		return this.player;
	}
	
	public void setOwner(Player p){
		this.player = p;
	}
	
}
